#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i;
    scanf("%d",&i);
    if(i>0){
        printf("The Given Number %d is Positive.",i);
    }
    else if(i<0){
        printf("The Given Number %d is Negative.",i);
    }else{
        printf("The Given Number 0 is Zero.");
    }
    return 0;
}
